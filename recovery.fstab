/efs			ext4		/dev/block/platform/dw_mmc/by-name/EFS		flags=display="EFS";backup=1
/boot			emmc		/dev/block/platform/dw_mmc/by-name/BOOT		flags=display="Boot";backup=1;
/recovery               emmc            /dev/block/platform/dw_mmc/by-name/RECOVERY	flags=display="Recovery";backup=1
/modem	                emmc            /dev/block/platform/dw_mmc/by-name/RADIO	flags=display="Modem";backup=1
/cache                  ext4            /dev/block/platform/dw_mmc/by-name/CACHE	flags=display="Cache";backup=1
/system                 ext4            /dev/block/platform/dw_mmc/by-name/SYSTEM	flags=display="System";backup=1
/data                   ext4            /dev/block/platform/dw_mmc/by-name/USERDATA	length=-32768	flags=backup=1
/misc                   emmc            /dev/block/platform/dw_mmc/by-name/OTA
/external_sdcard	vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="Micro SDCard";storage;wipeingui;removable
/usb-otg		vfat		/dev/block/sda1		/dev/block/sda		flags=display="USB-OTG";storage;wipeingui;removable

